    <!DOCTYPE html>
<html>
<head>
    <title>cekstatusptmt</title>
    <link rel="stylesheet" href="/public/Style/satpam/cekstatusptmt.css">
    <link rel="stylesheet" href="/public/font-awesome-4.7.0/css/font-awesome.min.css">
    <script src="/public/JS/daftarH.js"></script>
    <meta name="viewport" content="width=device-width, initial-scale=1">
</head>
<body>
    <div class="header">
        <div class="peteemte">
            <a id="home" href="/menu">PeTeEmTe SD Smarty</a>
        </div>

        <div class="tooltip">
            <a href="/" class="fa fa-power-off fa-3x logout"></a>
            <span class="tooltiptext"></span>
        </div>

    </div>

        <div class="background_menu">
            
            </div>
        </div>
        <div class="judul">
            <h1>Cek Status PTMT Siswa</h1>
            <form action="/get-states-by-country" method="post">
            <div class="NIS">
                <table>
                    <br>
                    <label for="country">Nomor Induk Siswa : </label>
                        <select class="form-control" id="country-dropdown">
                            <option value="">Nomor Induk Siswa</option>
                            <% if(data.length){
                                for(var i = 0; i< data.length; i++) {%> 
                                <option value="<%= data[i].id%>">
                                    <%= data[i].NIS%>
                                </option>
                                <% } } %>
                        </select>
                    </form>
                 </table>
             </div>   
            
            <div class="generate">
                <button type="button" id="btn_generate" onclick="goTo()">ok</button>
            </div> 
        </table>
        </div>
            <div class="icon">
                <br>
                <i class="fa fa-user-circle fa-5x" aria-hidden="true"></i>
        </div>
            
            <form action="/satpam" method="post">
            <div class="namasiswa">
                <% for (const row of satpam) {%>
                <h3>Nama Siswa : <%= row.nama_siswa %></h3> 
                Status PTMT : 
                <%= row.status_PTMT %>
                <% } %>
            </div>
        </form>
        <div class="vl"></div>

</body>
</html>